

  <div class="col-sm-6 col-md-4">
    <div class="ticket-cards-set">
      <div class="ticket-card text-center">
        <div class="heading">
          <h1><%= event.title %></h1>
          <p>@ <%= event.location %></p>
          <p>From <%= event.start_date %> til <%= event.end_date %></p>
          <h5><%= ticket.name %> | <%= ticket.price %></h5>
          <h5><%= ticket.user_email %></h5>
          <h2><%= ticket.formatted_sku %></h2>
        </div>
        <div class="content"><img src="/assets/logo_for_tickets.jpg" class="img-responsive" alt="Responsive image"></div>
      </div>
    </div>
  </div>
